{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true, // 不想要求import * as xxx from 'xxx'方式导入
    "strict": true,
    "noEmit": true,
  },
  "include": ["vite.config.ts"]
}
